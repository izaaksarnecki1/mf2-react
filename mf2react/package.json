{
  "name": "mf2react",
  "version": "0.1.1",
  "description": "MessageFormat v2 postProcessor for i18next/react-i18next",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@messageformat/core": "^3.4.0",
    "i18next": "^25.6.0",
    "react-i18next": "^16.0.0",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
